public Boolean companionPlants(String plant1, String plant2) {
    //code 
    if((plant1 == 'lettuce ' && plant2 == 'cucumbers')||(plant1 == 'lettuce' && plant2 == 'onions')||(plant1 == 'carrots' && plant2 == 'onions')||(plant1 == 'tomatoes' && plant2 == 'onions')){
        return true;

    }
    else{
        return false;
    }
}